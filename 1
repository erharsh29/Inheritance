class Car{  
void display()
{
    System.out.println("model: ferrari");
}  
}  
class ferrari extends Car{  
void color()
{
    System.out.println("Car color: Black");
}  
}  
class TestInheritance{  
public static void main(String args[]){  
ferrari f=new ferrari();  
f.display();  
f.color();  
}}  
